    "isCollapsedResultForSolr": true,
    "mltOptions": {
        "on": true,
        "fl": "artists,categoryNames,tags",
        "count": 20,
        "mintf": 1,
        "mindf": 5,
        "minwl": 3,
        "maxwl": 15,
        "qf": "id^1000"
    },
